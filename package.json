{
  "name": "prepr-valut-backend",
  "version": "1.0.0",
  "description": "Prepr Vault Backend API - Document and milestone management system",
  "scripts": {
    "build": "npm i --legacy-peer-deps && npm run build:compile && npm run build:assets",
    "build:compile": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --silent",
    "build:assets": "copyfiles --error -f package*.json dist/",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write && npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/MysqlDatasource.ts",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed-data.ts",
    "test": "jest",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "pm2dev": "pm2 restart ecosystem.config.js --env development && pm2 save --force",
    "pm2prod": "pm2 restart ecosystem.config.js --env production && pm2 save --force"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/client-textract": "^3.699.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "@tsed/ajv": "^7.67.4",
    "@tsed/async-hook-context": "^7.0.0-beta.9",
    "@tsed/common": "^7.87.3",
    "@tsed/core": "^7.67.4",
    "@tsed/di": "^7.87.3",
    "@tsed/engines": "^7.67.4",
    "@tsed/exceptions": "^7.67.4",
    "@tsed/json-mapper": "^7.67.4",
    "@tsed/logger": "^6.7.5",
    "@tsed/logger-file": "^6.7.5",
    "@tsed/openspec": "^7.67.4",
    "@tsed/platform-cache": "^7.67.4",
    "@tsed/platform-exceptions": "^7.67.4",
    "@tsed/platform-express": "^7.87.3",
    "@tsed/platform-log-middleware": "^7.67.4",
    "@tsed/platform-middlewares": "^7.67.4",
    "@tsed/platform-params": "^7.67.4",
    "@tsed/platform-response-filter": "^7.67.4",
    "@tsed/platform-views": "^7.67.4",
    "@tsed/schema": "^7.67.4",
    "@tsed/swagger": "^7.67.4",
    "ajv": "^8.12.0",
    "axios": "^1.6.8",
    "barrelsby": "^2.8.1",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "firebase-admin": "^12.0.0",
    "method-override": "^3.0.0",
    "mysql2": "^3.9.3",
    "sharp": "^0.33.5",
    "source-map-support": "^0.5.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "5.1.2",
    "@tsed/cli-plugin-typeorm": "5.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/method-override": "^0.0.35",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.5",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "http-status-codes": "^2.2.0",
    "jest": "^30.1.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.4"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "npm",
    "runtime": "node"
  }
}
